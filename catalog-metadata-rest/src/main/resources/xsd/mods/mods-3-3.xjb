<jxb:bindings version="2.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xsd="http://www.w3.org/2001/XMLSchema"
              xmlns:annox="http://annox.dev.java.net">

    <jxb:bindings schemaLocation="mods-3-3.xsd" node="/xsd:schema">
        <jxb:bindings node="//xsd:complexType[@name='modsType']">
            <annox:annotate target="class">
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement" name="mods" />
            </annox:annotate>
        </jxb:bindings>
        <jxb:bindings node="//xsd:complexType[@name='relatedItemType']">
            <jxb:bindings node=".//xsd:attribute[@name='type']">
                <jxb:property name="atType" />
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings node="//xsd:complexType[@name='nameType']">
            <jxb:bindings node=".//xsd:attribute[@name='type']">
                <jxb:property name="atType" />
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings node="//xsd:attributeGroup[@name='language']">
            <jxb:bindings node=".//xsd:attribute[@name='lang']">
                <jxb:property name="xmlLang" />
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings node="//xsd:extension[@base='baseTitleInfoType']">
            <jxb:bindings node=".//xsd:attribute[@name='type']">
                <jxb:property name="atType" />
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings node="//xsd:extension[@base='stringPlusDisplayLabel']">
            <jxb:bindings node=".//xsd:attribute[@name='type']">
                <jxb:property name="atType" />
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings node="//xsd:extension[@base='extensionType']">
            <jxb:bindings node=".//xsd:attribute[@name='type']">
                <jxb:property name="atType" />
            </jxb:bindings>
        </jxb:bindings>
        <jxb:bindings
                node="//xsd:complexType[@name='stringPlusAuthorityPlusTypePlusLanguage']/xsd:simpleContent/xsd:extension[@base='stringPlusAuthorityPlusLanguage']">
            <jxb:bindings node=".//xsd:attribute[@name='type']">
                <jxb:property name="atType" />
            </jxb:bindings>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>